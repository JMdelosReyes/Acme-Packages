<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd		
		http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-4.0.xsd		
	">

	<util:list id="converters">
		<bean class="converters.MessBoxToStringConverter" />
		<bean class="converters.StringToMessBoxConverter" />
		<bean class="converters.MessToStringConverter" />
		<bean class="converters.StringToMessConverter" />
		<bean class="converters.ActorToStringConverter" />
		<bean class="converters.StringToActorConverter" />
		<bean class="converters.CategoryToStringConverter" />
		<bean class="converters.StringToCategoryConverter" />
		<bean class="converters.TownToStringConverter" />
		<bean class="converters.StringToTownConverter" />
		<bean class="converters.FareToStringConverter" />
		<bean class="converters.StringToFareConverter" />
		<bean class="converters.VehicleToStringConverter" />
		<bean class="converters.StringToVehicleConverter" />
		<bean class="converters.SponsorshipToStringConverter" />
		<bean class="converters.StringToSponsorshipConverter" />
				
	</util:list>
	
</beans>